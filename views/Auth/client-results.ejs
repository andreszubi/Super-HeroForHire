<div class="client-search-page">
<nav class="navbar">
    <h3>We are here to make your life easier</h3>
    <a href="/auth/client/logout"><button class="pro-button" type="submit">Log out</button></a>
</nav>
<h1 class="title">Search results</h1>



<% professional.forEach(professional=> { %>
    <a href="auth/pro/pro-profile/<%- professional.id %>">
        <h2>
            <%= professional.firstname %>
                <%=professional.lastname %>
        </h2>
    </a>
    <h3>
        <%= professional.city %>
    </h3>
    <h3>
        <%= professional.postalcode %>
    </h3>
    <h3>
        <%= professional.services %>
    </h3>
    <h3>
        <%= professional.price %>
    </h3>
    <a href="/auth/client/booking-confirmation"><button type="submit">Book</button></a>

    <% }) %>

</div>
